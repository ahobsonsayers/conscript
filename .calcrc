define summ(args) {
  local sum = 0;
  for (local i = 1; i <= args; i++) {
    sum = sum + arg(i);
  }
  return sum;
}

define sc() 
 { 
         local s, i; 
  
         s = 0; 
         for (i = 1; i <= param(0); i++) { 
                 if (!isnum(param(i))) { 
                         print "parameter",i,"is not a number"; 
                         continue; 
                 } 
                 s += param(i)^3; 
         } 
         return s; 
 }
